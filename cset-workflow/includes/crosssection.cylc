{% if EXTRACT_CROSS_SECTION %}
[scheduling]
    [[graph]]
    P1 = """
    recipe_start => crosssection => recipe_finish
    """

[runtime]
    [[crossection]]
    script = rose task-run -v --app-key=run_cset_recipe
    execution time limit = PT60M
        [[[environment]]]
        CSET_RECIPE_NAME = "crosssection.yaml"
        CSET_ADDOPTS = "--CS_STARTCOORDS={{CS_STARTCOORDS}} --CS_FINISHCOORDS={{CS_FINISHCOORDS}} --CS_VARS={{CS_VARS}}"
{% endif %}
